{
  "name": "ubiquitab_db",
  "version": "1.0.0",
  "description": "Interface with ubiquitab's database",
  "main": "index.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreraa/ubiquitabDB_Utils.git"
  },
  "keywords": [
    "node.js",
    "dynamodb",
    "ubiquitab"
  ],
  "author": "ferreraa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ferreraa/ubiquitabDB_Utils/issues"
  },
  "homepage": "https://github.com/ferreraa/ubiquitabDB_Utils#readme",
  "dependencies": {
    "@tsconfig/node12": "^1.0.7",
    "aws-sdk": "^2.853.0",
    "dotenv": "^8.2.0",
    "dynamodb-data-types": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.2"
  }
}
